{
  "name": "@white-rabbit/desktop-render",
  "private": true,
  "version": "0.0.1",
  "description": "The desktop render for Wonderland WhiteRabbit",
  "author": "Ukonn Ra <ukonnra@outlook.com>",
  "homepage": "https://github.com/alices-wonderland/white-rabbit",
  "license": "MIT",
  "scripts": {
    "dev": "vite --port 4000",
    "build:desktop": "tsc && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "prettier --check \"./**/*.{ts,vue,json}\" && eslint .",
    "lint:fix": "prettier --write --check \"./**/*.{ts,vue,json}\" && eslint . --fix"
  },
  "dependencies": {
    "pinia": "^2.0.11",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^9.5.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.5.0",
    "jsdom": "^19.0.0",
    "start-server-and-test": "^1.14.0",
    "vite": "^2.8.6",
    "vitest": "^0.6.0",
    "vue-tsc": "^0.32.1"
  }
}
