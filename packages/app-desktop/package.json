{
  "name": "@white-rabbit/app-desktop",
  "packageManager": "yarn@3.2.0",
  "version": "0.0.1",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "clean": "rimraf dist release app/.yarn app/dist app/node_modules",
    "build": "yarn clean && ts-node build.ts",
    "dev:electron": "electron .",
    "dev": "yarn build --watch",
    "lint": "eslint . --ext .vue,.js,.ts --ignore-path ../../.gitignore",
    "lint:fix": "yarn lint --fix",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@white-rabbit/business-logic": "workspace:^",
    "@white-rabbit/desktop-render": "workspace:^",
    "@white-rabbit/repository-memory": "workspace:^",
    "@white-rabbit/repository-sequelize": "workspace:^",
    "dayjs": "^1.11.1",
    "electron-is-dev": "^2.0.0",
    "express": "^4.18.1",
    "js-base64": "^3.7.2",
    "pinia": "^2.0.13",
    "sequelize": "^6.19.0",
    "sqlite3": "^5.0.7",
    "tsyringe": "^4.6.0",
    "uuid": "^8.3.2",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "electron": "18.2.0",
    "electron-builder": "^23.0.8"
  }
}
