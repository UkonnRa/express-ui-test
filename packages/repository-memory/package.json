{
  "name": "@white-rabbit/repository-memory",
  "packageManager": "yarn@3.2.1",
  "version": "0.0.1",
  "description": "Wonderland WhiteRabbit Repository Implementation: In Memory",
  "author": "Ukonn Ra<ukonnra@outlook.com>",
  "license": "MIT",
  "private": true,
  "main": "src/index.ts",
  "publishConfig": {
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo tsconfig.dev.tsbuildinfo",
    "build": "yarn clean && tsc -b && ts-node esbuild.config.ts",
    "lint": "eslint . --ext .vue,.js,.ts --ignore-path ../../.gitignore",
    "lint:fix": "yarn lint --fix"
  },
  "peerDependencies": {
    "@white-rabbit/business-logic": "workspace:^*",
    "js-base64": "*",
    "tsyringe": "*",
    "uuid": "*",
    "winston": "*"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.4",
    "@white-rabbit/test-suite": "workspace:^*",
    "reflect-metadata": "^0.1.13"
  }
}
