{
  "name": "@white-rabbit/components",
  "packageManager": "yarn@3.2.0",
  "version": "0.0.1",
  "main": "src/index.ts",
  "publishConfig": {
    "main": "dist/components.umd.js",
    "module": "dist/components.es.js"
  },
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "lint": "yarn lint:typecheck && eslint . --ext .vue,.js,.ts --ignore-path ../../.gitignore",
    "lint:fix": "yarn lint --fix",
    "lint:typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "test": "cross-env NODE_ENV=test vitest run --coverage",
    "storybook": "start-storybook -p 5021 -c ../../config/storybook",
    "build:storybook": "build-storybook -c ../../config/storybook",
    "preview:storybook": "serve storybook-static -p 5020"
  },
  "peerDependencies": {
    "@vueuse/components": "*",
    "@vueuse/core": "*",
    "uuid": "*"
  }
}
