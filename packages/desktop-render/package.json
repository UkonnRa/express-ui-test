{
  "name": "@white-rabbit/desktop-render",
  "packageManager": "yarn@3.2.0",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite --port 5030",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5030",
    "lint": "yarn lint:typecheck && eslint . --ext .vue,.js,.ts --ignore-path ../../.gitignore",
    "lint:fix": "yarn lint --fix",
    "lint:typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "test": "cross-env NODE_ENV=test vitest run --coverage",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5030/ 'cypress run'",
    "storybook": "start-storybook -p 5031 -c ../../config/storybook",
    "build-storybook": "build-storybook -c ../../config/storybook"
  },
  "dependencies": {
    "@mdi/font": "6.6.96",
    "@vueuse/components": "^8.3.1",
    "@vueuse/core": "^8.3.1",
    "@white-rabbit/components": "workspace:^",
    "pinia": "^2.0.13",
    "roboto-fontface": "^0.10.0",
    "uuid": "^8.3.2",
    "vue": "^3.2.33",
    "vue-i18n": "9.1.9",
    "vuetify": "^3.0.0-beta.1",
    "webfontloader": "^1.6.28"
  }
}
