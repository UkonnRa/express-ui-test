{
  "name": "@white-rabbit/app-backend-grpc",
  "packageManager": "yarn@3.2.1",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "clean": "rimraf src/proto dist protoset.bin",
    "build": "yarn compile:proto && ts-node esbuild.config.ts",
    "preview": "node dist/index.js",
    "compile:proto": "yarn clean; mkdirp src/proto; yarn compile:proto:ts",
    "compile:proto:ts": "grpc_tools_node_protoc --ts_proto_out=src/proto --ts_proto_opt=outputServices=nice-grpc,outputServices=generic-definitions,esModuleInterop=true --descriptor_set_out=./protoset.bin --include_imports  --proto_path=../../schemas/proto ../../schemas/proto/*.proto",
    "lint": "eslint . --ext .vue,.js,.ts --ignore-path ../../.eslintignore",
    "lint:fix": "yarn lint --fix",
    "orm:seed": "ts-node seed.ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@mikro-orm/core": "^5.1.5",
    "@mikro-orm/postgresql": "^5.1.5",
    "@white-rabbit/business-logic": "workspace:^",
    "long": "^5.2.0",
    "nice-grpc": "^1.2.0",
    "nice-grpc-error-details": "^0.1.0",
    "nice-grpc-server-reflection": "^1.0.12",
    "protobufjs": "^6.11.3",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.1.5",
    "@mikro-orm/reflection": "^5.1.5",
    "@mikro-orm/seeder": "^5.1.5",
    "cross-env": "^7.0.3",
    "grpc-tools": "^1.11.2",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "ts-proto": "^1.115.4"
  }
}
