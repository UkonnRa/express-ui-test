{
  "name": "@white-rabbit/app-frontend",
  "packageManager": "yarn@3.2.0",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite --port 5010",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5010",
    "lint": "eslint . --ext .vue,.js,.ts --ignore-path ../../.gitignore",
    "lint:fix": "yarn lint --fix",
    "test": "echo 'Should not run `vitest run --coverage`'",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5010/ 'cypress run'",
    "storybook": "start-storybook -p 5011 -c ../../config/storybook",
    "build-storybook": "build-storybook -c ../../config/storybook"
  },
  "dependencies": {
    "@mdi/font": "6.6.95",
    "@white-rabbit/components": "workspace:^",
    "roboto-fontface": "^0.10.0",
    "vue": "^3.2.31",
    "vuetify": "^3.0.0-beta.0",
    "webfontloader": "^1.6.28"
  }
}
