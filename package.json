{
  "name": "white-rabbit",
  "packageManager": "yarn@3.2.0",
  "version": "0.0.1",
  "engines": {
    "node": ">=16",
    "yarn": ">=3"
  },
  "scripts": {
    "prepare": "husky install",
    "clean": "yarn workspaces foreach -pit run clean",
    "build": "yarn lint && yarn workspaces foreach -pit run build",
    "dev": "yarn workspaces foreach -pit run dev",
    "lint:markdown": "markdownlint -p .markdownlint.json README.md packages/*/README.md docs/docs/*.md",
    "lint:prettier": "prettier --no-config --check \"./**/*.{ts,vue,js,json}\" --ignore-path .gitignore",
    "lint": "yarn lint:markdown && yarn lint:prettier && yarn workspaces foreach -pit run lint",
    "lint:fix": "yarn lint:prettier --write && yarn workspaces foreach -pit run lint:fix",
    "test": "yarn workspaces foreach -pit run test",
    "test:e2e": "yarn workspaces foreach -it run test:e2e"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@rushstack/eslint-patch": "^1.1.1",
    "@types/eslint": "^8.4.1",
    "@types/jest": "^27.4.1",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.23",
    "@types/webfontloader": "^1.6.34",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/cli": "^5.0.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/tsconfig": "^0.1.3",
    "@vuetify/vite-plugin": "^1.0.0-alpha.10",
    "c8": "^7.11.0",
    "chokidar": "^3.5.3",
    "commitlint": "^16.2.3",
    "cypress": "^9.5.2",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.14.28",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sonarjs": "^0.12.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-each": "^27.5.1",
    "jsdom": "^19.0.0",
    "markdownlint-cli": "^0.31.1",
    "prettier": "^2.6.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "start-server-and-test": "^1.14.0",
    "tree-kill": "^1.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "vite": "^2.8.6",
    "vitest": "^0.7.11",
    "vue": "^3.2.31",
    "vue-cli-plugin-vuetify": "^2.4.8",
    "vue-tsc": "^0.33.9",
    "vuetify-loader": "^1.7.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
