{
  "name": "white-rabbit",
  "packageManager": "yarn@3.2.1",
  "version": "0.0.1",
  "engines": {
    "node": ">=18",
    "yarn": ">=3"
  },
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "clean": "yarn workspaces foreach -pit run clean",
    "build": "yarn lint && yarn workspaces foreach -pit run build",
    "dev": "yarn workspaces foreach -pit run dev",
    "lint:style": "stylelint \"**/*.{vue,scss,css}\"",
    "lint:markdown": "markdownlint -p .markdownlint.json README.md packages/**/*.md",
    "lint:prettier": "prettier --no-config --check \"{**/*,*}.{js,ts,jsx,tsx,vue,json}\"",
    "lint:eslint": "eslint .eslintrc.backend.js .eslintrc.js .eslintrc.vue.js commitlint.config.js esbuild.config.ts jest.config.ts vitest.setup.ts",
    "lint": "yarn lint:eslint && yarn lint:markdown && yarn lint:style && yarn lint:prettier && yarn workspaces foreach -pit run lint",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:markdown --fix && yarn lint:prettier --write && yarn lint:style --fix && yarn workspaces foreach -pit run lint:fix",
    "test": "yarn workspaces foreach -pit run test",
    "test:e2e": "yarn workspaces foreach -it run test:e2e"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@digitalroute/cz-conventional-changelog-for-jira": "^7.3.0",
    "@faker-js/faker": "^7.2.0",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@mdx-js/react": "^2.1.1",
    "@storybook/addon-actions": "^6.5.7",
    "@storybook/addon-docs": "^6.5.7",
    "@storybook/addon-essentials": "^6.5.7",
    "@storybook/addon-links": "^6.5.7",
    "@storybook/builder-vite": "^0.1.36",
    "@storybook/core-common": "^6.5.7",
    "@storybook/vue3": "^6.5.7",
    "@types/eslint": "^8.4.2",
    "@types/jest": "^28.1.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.40",
    "@types/webfontloader": "^1.6.34",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0",
    "babel-loader": "^8.2.5",
    "c8": "^7.11.3",
    "chokidar": "^3.5.3",
    "commitizen": "^4.2.4",
    "commitlint": "^17.0.2",
    "cross-env": "^7.0.3",
    "cypress": "^10.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.14.42",
    "esbuild-visualizer": "^0.3.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-vue": "^9.1.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-each": "^28.1.0",
    "jsdom": "^19.0.0",
    "markdownlint-cli": "^0.31.1",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.52.2",
    "sass-loader": "^13.0.0",
    "serve": "^13.0.2",
    "stylelint": "^14.8.5",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "vite": "^2.9.10",
    "vite-plugin-vuetify": "^1.0.0-alpha.11",
    "vitest": "^0.14.1",
    "vue-eslint-parser": "^9.0.2",
    "vue-tsc": "^0.37.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira"
    }
  },
  "resolutions": {
    "minimist": "^1.2.6",
    "trim": "^1.0.0",
    "trim-newlines": "^3.0.1"
  }
}
