{
  "name": "white-rabbit",
  "version": "0.0.1",
  "description": "Wonderland WhiteRabbit root project",
  "author": "Ukonn Ra<ukonnra@outlook.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "test": "jest --coverage --passWithNoTests",
    "lint": "prettier --check \"./**/*.{ts,tsx,js,json,md,mdx}\" && eslint . --ext .js,.ts,.tsx && npm run lint:markdown",
    "lint:fix": "prettier --write \"./**/*.{ts,tsx,js,json,md,mdx}\" && eslint . --fix --ext .js,.ts,.tsx && npm run lint:markdown --fix",
    "lint:markdown": "markdownlint -p .markdownlint.json README.md packages/*/README.md services/*/README.md docs/docs/*.{md,mdx}",
    "clean": "rimraf packages/*/dist/ services/*/dist/ services/desktop/release/",
    "clean:all": "npm run clean && rimraf node_modules/ services/*/node_modules/",
    "dev:desktop": "ts-node scripts/dev-desktop.ts",
    "build": "npm run clean && cross-env NODE_ENV=production npm run build --workspaces --if-present && ts-node scripts/build-desktop.ts",
    "prepare": "husky install",
    "dependencies:update": "npx npm-check-updates -ui -x rehype-katex,remark-math,@mdx-js/react --deep --dep prod,dev,peer,bundle,optional"
  },
  "workspaces": [
    "packages/*",
    "services/*"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-env": "^1.16.3",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.12.0",
    "eslint-plugin-testing-library": "^5.0.6",
    "fs-extra": "^10.0.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-each": "^27.5.1",
    "markdownlint-cli": "^0.31.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "run-script-webpack-plugin": "^0.0.11",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.7",
    "ts-node": "^10.6.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-node-externals": "^3.0.0"
  }
}
