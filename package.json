{
  "name": "white-rabbit",
  "packageManager": "yarn@3.2.1",
  "version": "0.0.1",
  "engines": {
    "node": ">=18",
    "yarn": ">=3"
  },
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "clean": "yarn workspaces foreach -pit run clean",
    "build": "yarn lint && yarn workspaces foreach -pit run build",
    "dev": "yarn workspaces foreach -pit run dev",
    "lint:markdown": "markdownlint -p .markdownlint.json README.md packages/*/README.md docs/docs/*.md",
    "lint:prettier": "prettier --no-config --check \"./**/*.{ts,vue,js,json}\" --ignore-path .gitignore",
    "lint": "yarn lint:markdown && yarn lint:prettier && yarn workspaces foreach -pit run lint",
    "lint:fix": "yarn lint:prettier --write && yarn workspaces foreach -pit run lint:fix",
    "test": "yarn workspaces foreach -pit run test",
    "test:e2e": "yarn workspaces foreach -it run test:e2e",
    "commit:prepare": "yarn test && yarn lint:fix && git add .",
    "commit:amend": "yarn commit:prepare && git commit --amend --no-edit",
    "commit:cz": "yarn commit:prepare && git cz"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@faker-js/faker": "^7.0.1",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@mdx-js/react": "^2.1.1",
    "@rushstack/eslint-patch": "^1.1.3",
    "@storybook/addon-actions": "^6.5.4",
    "@storybook/addon-docs": "^6.5.4",
    "@storybook/addon-essentials": "^6.5.4",
    "@storybook/addon-links": "^6.5.4",
    "@storybook/builder-vite": "^0.1.35",
    "@storybook/core-common": "^6.5.4",
    "@storybook/vue3": "^6.5.4",
    "@types/eslint": "^8.4.2",
    "@types/jest": "^27.5.1",
    "@types/jsdom": "^16.2.14",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.35",
    "@types/webfontloader": "^1.6.34",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/cli": "^5.0.4",
    "@vue/compiler-sfc": "^3.2.36",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "babel-loader": "^8.2.5",
    "c8": "^7.11.3",
    "chokidar": "^3.5.3",
    "commitlint": "^17.0.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.7.0",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.14.39",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-vue": "^9.0.1",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-each": "^28.1.0",
    "jsdom": "^19.0.0",
    "less": "^4.1.2",
    "markdownlint-cli": "^0.31.1",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.74.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "serve": "^13.0.2",
    "start-server-and-test": "^1.14.0",
    "tree-kill": "^1.2.2",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.8.1",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.9.9",
    "vitest": "^0.13.0",
    "vscode-languageserver-protocol": "^3.17.1",
    "vue": "^3.2.36",
    "vue-loader": "^17.0.0",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.34.16",
    "webpack": "^5.72.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "async": "^3.2.2",
    "minimist": "^1.2.6",
    "trim": "^1.0.0",
    "trim-newlines": "^4.0.0"
  }
}
