{
  "name": "white-rabbit",
  "packageManager": "yarn@3.2.0",
  "version": "0.0.1",
  "engines": {
    "node": ">=16",
    "yarn": ">=3"
  },
  "scripts": {
    "prepare": "husky install",
    "clean": "yarn workspaces foreach -pit run clean",
    "build": "yarn lint && yarn workspaces foreach -pit run build",
    "dev": "yarn workspaces foreach -pit run dev",
    "lint:markdown": "markdownlint -p .markdownlint.json README.md packages/*/README.md docs/docs/*.md",
    "lint:prettier": "prettier --no-config --check \"./**/*.{ts,vue,js,json}\" --ignore-path .gitignore",
    "lint": "yarn lint:markdown && yarn lint:prettier && yarn workspaces foreach -pit run lint",
    "lint:fix": "yarn lint:prettier --write && yarn workspaces foreach -pit run lint:fix",
    "test": "yarn workspaces foreach -pit run test",
    "test:e2e": "yarn workspaces foreach -it run test:e2e"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@mdx-js/react": "^2.1.1",
    "@rushstack/eslint-patch": "^1.1.3",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-docs": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-vite": "^0.1.32",
    "@storybook/core-common": "^6.4.22",
    "@storybook/vue3": "^6.4.22",
    "@types/eslint": "^8.4.1",
    "@types/jest": "^27.4.1",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.30",
    "@types/webfontloader": "^1.6.34",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/cli": "^5.0.4",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.21",
    "@vue/tsconfig": "^0.1.3",
    "babel-loader": "^8.2.5",
    "c8": "^7.11.2",
    "chokidar": "^3.5.3",
    "commitlint": "^16.2.4",
    "cross-env": "^7.0.3",
    "cypress": "^9.6.0",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.14.38",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.4",
    "jest": "^28.0.3",
    "jest-each": "^28.0.2",
    "jsdom": "^19.0.0",
    "less": "^4.1.2",
    "markdownlint-cli": "^0.31.1",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "serve": "^13.0.2",
    "start-server-and-test": "^1.14.0",
    "tree-kill": "^1.2.2",
    "ts-jest": "^28.0.0-next.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^2.9.6",
    "vitest": "^0.10.0",
    "vscode-languageserver-protocol": "^3.16.0",
    "vue": "^3.2.33",
    "vue-loader": "^17.0.0",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.34.11",
    "webpack": "^5.72.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "async": "^3.2.2",
    "minimist": "^1.2.6",
    "trim": "^0.0.3"
  }
}
